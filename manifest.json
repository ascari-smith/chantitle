{
    "manifest_version": 2,
    "name": "Chantitle",
    "version": "1.0",
    "description": "Change the title of tabs quickly and easily.", 
    "browser_action": {
        "default_icon": "img/icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "content_scripts": [
        {
            "matches":["http://*/*", "https://*/*"],
            "js": ["js/popup.js"]
        }
    ]
}
