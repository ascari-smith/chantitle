{
    "manifest_version": 2,
    "name": "Chantitle",
    "version": "1.0",
    "description": "Change the title of tabs quickly and easily.",
    "homepage_url": "https://github.com/ascari-smith/chantitle", 
    "browser_action": {
        "default_icon": "img/icon.png",
        "default_popup": "chantitle.html"
    },
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "content_scripts": [
        {
            "matches": ["http://none/*"],
            "js": ["js/chantitle.js"]
        }
    ]
}
